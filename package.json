{
  "name": "potrico-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "npx prisma migrate deploy && npx ts-node src/server.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.0.0",
    "axios": "1.5.0",
    "body-parser": "1.20.2",
    "express": "4.18.2",
    "openai": "4.10.0",
    "form-data": "4.0.0",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "prisma": "5.0.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12"
  }
}
